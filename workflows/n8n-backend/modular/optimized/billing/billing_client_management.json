          "__rl": true,
          "value": "clients",
          "mode": "list"
        },
        "filterByFormula": "=OR({siteSlug}='{{ $json.siteSlug }}', {email}='{{ $json.email }}')",
        "options": {}
      },
      "id": "node_3_airtable-search-client-plan",
