    },
    {
      "parameters": {
        "operation": "list",
        "base": {
          "__rl": true,
          "value": "appELEVEA",
          "mode": "list"
        },
        "table": {
          "__rl": true,
          "value": "whatsapp_messages",
          "mode": "list"
        },
        "limit": "={{ $json.limit }}",
        "options": {
          "sort": [
