{
  "name": "whatsapp_api_whatsapp_send_ai",
  "description": "Workflow modular para api/whatsapp/send-ai - M√≥dulo WHATSAPP",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
