{
  "name": "billing_api_admin_toggle",
  "description": "Workflow modular para api/admin/toggle - M√≥dulo BILLING",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "api/admin/toggle",
        "responseMode": "lastNode",
        "options": {}
      },
      "id": "webhook-admin-toggle",
      "name": "üîß Webhook - Admin Toggle",
