      "name": "üìë Code - Sections Upsert",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        460,
        7700
      ]
    },
    {
      "parameters": {
        "operation": "search",
        "base": {
          "__rl": true,
          "value": "appELEVEA",
